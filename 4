#include "stdafx.h"
#include <iostream>
#include <string>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	string str,c= "aouey";
	cout << "Введите строку: ";
	getline(cin,str);
	//cin >> str;
	int n = str.size(),x=0;
	for (int i = 0;i < n;i++) {
		for (int j = 0;j < 3;j++) {
			if (c[j] == str[i]) {
				x++;
			}
		}
	}
	cout <<"Гласных: "<< x << endl;
}
